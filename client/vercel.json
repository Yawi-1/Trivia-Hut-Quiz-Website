{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "http://localhost:3001/api/$1"
      }
    ],
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  